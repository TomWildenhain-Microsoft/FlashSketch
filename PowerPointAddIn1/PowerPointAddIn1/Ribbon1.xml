<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
	<ribbon>
		<tabs>
			<tab idMso="TabAddIns">
				<group id="ContentGroup" label="FlashSketch">
					<button id="newArtboardButton" label="Equalize Widths"
						 screentip="Text" onAction="OnEqualizeWidths" imageMso="SizeToControlWidth"
						 supertip="Creates a new Artboard"/>
					<button id="newArtboardButton2" label="Equalize Heights"
						 screentip="Text" onAction="OnEqualizeHeights" imageMso="SizeToControlHeight"
						 supertip="Creates a new Artboard"/>
					<button id="newArtboardButton3" label="Make Square"
						 screentip="Text" onAction="OnMakeSquare" imageMso="SizeToControlHeightAndWidth"
						 supertip="Creates a new Artboard"/>
					<button id="newArtboardButton4" label="Distribute Horizontally"
						 screentip="Text" onAction="OnDistributeHorizontally" imageMso="AlignDistributeHorizontally"
						 supertip="Creates a new Artboard"/>
					<button id="newArtboardButton5" label="Distribute Vertically"
						 screentip="Text" onAction="OnDistributeVertically" imageMso="AlignDistributeVertically"
						 supertip="Creates a new Artboard"/>
					<button id="newArtboardButton6" label="Equalize Line Lengths"
						 screentip="Text" onAction="OnEqualizeLineLengths" imageMso="ControlLine"
						 supertip="Creates a new Artboard"/>
					<button id="newArtboardButton10" label="New Artboard"
						 screentip="Text" onAction="OnNewArtboard" imageMso="ShadowOptionsDialog"
						 supertip="Creates a new Artboard"/>
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>

