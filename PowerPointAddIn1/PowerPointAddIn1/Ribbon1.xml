<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
	<ribbon>
		<tabs>
			<tab idMso="TabAddIns">
				<group id="ContentGroup" label="SmartAlign">
					<button id="newArtboardButton11" label="Recompute Constraints"
						 screentip="Recompute Constraints" onAction="OnRecompute" imageMso="Refresh"
						 supertip="Recompute Constraints"/>
					<button id="newArtboardButton" label="Equalize Widths"
						 screentip="Equalize Widths" onAction="OnEqualizeWidths" imageMso="SizeToControlWidth"
						 supertip="Equalize Widths"/>
					<button id="newArtboardButton2" label="Equalize Heights"
						 screentip="Equalize Heights" onAction="OnEqualizeHeights" imageMso="SizeToControlHeight"
						 supertip="Equalize Heights"/>
					<button id="newArtboardButton3" label="Make Square"
						 screentip="Make Square" onAction="OnMakeSquare" imageMso="SizeToControlHeightAndWidth"
						 supertip="Make Square"/>
					<button id="newArtboardButton4" label="Distribute Horizontally"
						 screentip="Distribute Horizontally" onAction="OnDistributeHorizontally" imageMso="AlignDistributeHorizontally"
						 supertip="Distribute Horizontally"/>
					<button id="newArtboardButton5" label="Distribute Vertically"
						 screentip="Distribute Vertically" onAction="OnDistributeVertically" imageMso="AlignDistributeVertically"
						 supertip="Distribute Vertically"/>
					<button id="newArtboardButton6" label="Equalize Line Lengths"
						 screentip="Equalize Line Lengths" onAction="OnEqualizeLineLengths" imageMso="ControlLine"
						 supertip="Equalize Line Lengths"/>
					<toggleButton id="newArtboardButton12" label="Lock Constraints" onAction="OnLockConstraints" getPressed="GetPressedLockConstraints"
						 screentip="Lock Constraints" imageMso="Lock"
						 supertip="Lock Constraints"/>
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>

